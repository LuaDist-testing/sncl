region rgFullScreen
	height = 100%
	width = 100%
	region rgHalfScreenLeft
		height = 50%
		width = 50%
		left = 0
	end
	region rgHalfScreenRight
		height = 50%
		width = 50%
		right = 0
	end
end

context context1
	port pContext1 media1Photo
	media media1Photo
		region = rgHalfScreenLeft
		src = ../media/photo1.jpg
		focusIndex = 1
	end
	media media1Text
		region = rgHalfScreenRight
		src = ../media/text1.html
		focusIndex = 1
	end
end

